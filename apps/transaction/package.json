      "jest": "^27.5.1",
      "prettier": "^2.8.8",
      "source-map-support": "^0.5.21",
      "supertest": "^6.3.4",
      "ts-jest": "^27.1.5",
      "ts-loader": "^9.5.1",
      "ts-node": "10.9.1",
      "tsconfig-paths": "^3.15.0",
      "typescript": "4.5.2"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".*\\.spec\\.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "collectCoverageFrom": [
         "**/*.(t|j)s"
      ],
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
